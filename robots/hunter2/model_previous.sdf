<?xml version="1.0"?>
<sdf version="1.6">
  <world name="default">

    <include>
      <uri>model://ground_plane</uri>
    </include>

    <include>
      <uri>model://sun</uri>
    </include>

    <scene>
      <shadows>false</shadows>
    </scene>

    <gui fullscreen='0'>
      <camera name='user_camera'>
        <pose frame='chassis_link'>0.319654 -0.235002 9.29441 0 1.5138 0.009599</pose>
        <view_controller>orbit</view_controller>
        <projection_type>perspective</projection_type>
      </camera>
    </gui>

    <physics type="ode">
      <real_time_update_rate>1000.0</real_time_update_rate>
      <max_step_size>0.001</max_step_size>
      <real_time_factor>1</real_time_factor>
      <ode>
        <solver>
          <type>quick</type>
          <iters>150</iters>
          <precon_iters>0</precon_iters>
          <sor>1.400000</sor>
          <use_dynamic_moi_rescaling>1</use_dynamic_moi_rescaling>
        </solver>
        <constraints>
          <cfm>0.00001</cfm>
          <erp>0.2</erp>
          <contact_max_correcting_vel>2000.000000</contact_max_correcting_vel>
          <contact_surface_layer>0.01000</contact_surface_layer>
        </constraints>
      </ode>
    </physics>

    <!--model name="turtlebot3_world_big">
      <static>1</static>
      <include>
        <uri>model://turtlebot3_world_big</uri>
      </include>
    </model-->
    
    <model name="hunter_v2"> 
      <pose>0.0 0.0 0.7 0.0 0.0 0.0</pose>
      
      <link name="base_link"/>
      
      <joint name="chassis_joint" type="fixed">
        <parent>base_link</parent>
        <child>chassis_link</child>
        <pose>0.4775 0 0 0 0 0</pose>
      </joint>

      
      <link name="chassis_link">

        <inertial>
          <pose>0 0 0 0 0 0</pose>
          <inertia>
            <ixx>2.288641</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>5.103976</iyy>
            <iyz>0</iyz>
            <izz>3.431465</izz>
          </inertia>
          <mass>40.0</mass>
        </inertial>

        <collision name="collision_1">
          <pose>0 0 0 0 0 0</pose>
          <geometry>
            <box>
              <size>0.76 0.27 0.270</size>
            </box>
          </geometry>
        </collision>
        
        
        <visual name="visual_1">
          <pose>0 0 0 0 0 0</pose>
          <geometry>
            <box>
              <size>0.76 0.27 0.270</size>
            </box>
          </geometry>
        </visual>
        
      </link>
      
      <!--rear left wheel-->
      <link name="rear_left_wheel_link">
      
        <inertial>
          <pose>0 0 0 0 0 0</pose>
          <inertia>
            <ixx>0.7171</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.1361</iyy>
            <iyz>0</iyz>
            <izz>0.7171</izz>
          </inertia>
          <mass>3.0</mass>
        </inertial>
        
        
        <collision name="rear_left_wheel_collision">
          <pose>-0.4775 0.2325 0 -1.5707 0 0</pose>
          <geometry>
            <cylinder>
              <radius>0.165</radius>
              <length>0.140</length>
            </cylinder>
          </geometry>
        </collision>
        
        <visual name="rear_left_wheel_visual">
          <pose>-0.4775 0.2325 0 -1.5707 0 0</pose>
          <geometry>
            <cylinder>
              <radius>0.165</radius>
              <length>0.140</length>
            </cylinder>
          </geometry>
        </visual>
      </link>
      
      <joint name="rear_left_wheel_joint" type="revolute">
        <parent>chassis_link</parent>
        <child>rear_left_wheel_link</child>
        <pose>-0.4775 0.2325 0 0 0 0</pose>
        <axis>
         <xyz>0 1 0</xyz>
        </axis>
      </joint>
      
      
      <!--rear right wheel-->
      <link name="rear_right_wheel_link">
      
        <inertial>
          <pose>0 0 0 0 0 0</pose>
          <inertia>
            <ixx>0.7171</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.1361</iyy>
            <iyz>0</iyz>
            <izz>0.7171</izz>
          </inertia>
          <mass>3.0</mass>
        </inertial>
        
        
        <collision name="rear_right_wheel_collision">
          <pose>-0.4775 -0.2325 0 -1.5707 0 0</pose>
          <geometry>
            <cylinder>
              <radius>0.165</radius>
              <length>0.140</length>
            </cylinder>
          </geometry>
        </collision>
        
        <visual name="rear_right_wheel_visual">
          <pose>-0.4775 -0.2325 0 -1.5707 0 0</pose>
          <geometry>
            <cylinder>
              <radius>0.165</radius>
              <length>0.140</length>
            </cylinder>
          </geometry>
        </visual>
      </link>
      
      <joint name="rear_right_wheel_joint" type="revolute">
        <parent>chassis_link</parent>
        <child>rear_right_wheel_link</child>
        <pose>-0.4775 -0.2325 0 0 0 0</pose>
        <axis>
         <xyz>0 1 0</xyz>
        </axis>
      </joint>
      
      <!--front right-->
      <joint name="front_right_steer_joint" type="revolute">
        <parent>chassis_link</parent>
        <child>front_right_steer_link</child>
        <pose>0.4775 -0.2325 0 0 0 0</pose>
        <axis>
         <xyz>0 0 1</xyz>
         <limit>
           <lower>-0.6</lower>
           <upper>0.6</upper>
         </limit>
        </axis>
      </joint>
      
      <link name="front_right_steer_link"/>
      
            
      <joint name="front_right_wheel_joint" type="revolute">
        <parent>front_right_steer_link</parent>
        <child>front_right_wheel_link</child>
        <pose>0.4775 -0.2325 0 0 0 0</pose>
        <axis>
         <xyz>0 1 0</xyz>
        </axis>
      </joint>
      
    
      <link name="front_right_wheel_link">
      
        <inertial>
          <pose>0 0 0 0 0 0</pose>
          <inertia>
            <ixx>0.007171</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.001361</iyy>
            <iyz>0</iyz>
            <izz>0.007171</izz>
          </inertia>
          <mass>0.1</mass>
        </inertial>
        
        
        <collision name="front_right_wheel_collision">
          <pose>0.4775 -0.2325 0 -1.5707 0 0</pose>
          <geometry>
            <cylinder>
              <radius>0.165</radius>
              <length>0.140</length>
            </cylinder>
          </geometry>
        </collision>
        
        <visual name="front_right_wheel_visual">
          <pose>0.4775 -0.2325 0 -1.5707 0 0</pose>
          <geometry>
            <cylinder>
              <radius>0.165</radius>
              <length>0.140</length>
            </cylinder>
          </geometry>
        </visual>
      </link>
      
       
      <!--front left-->
      <joint name="front_left_steer_joint" type="revolute">
        <parent>chassis_link</parent>
        <child>front_left_steer_link</child>
        <pose>0.4775 0.2325 0 0 0 0</pose>
        <axis>
         <xyz>0 0 1</xyz>
         <limit>
           <lower>-0.6</lower>
           <upper>0.6</upper>
         </limit>
        </axis>
      </joint>
      
      <link name="front_left_steer_link"/>
      
      <joint name="front_left_wheel_joint" type="revolute">
        <parent>front_left_steer_link</parent>
        <child>front_left_wheel_link</child>
        <pose>0.4775 0.2325 0 0 0 0</pose>
        <axis>
         <xyz>0 1 0</xyz>
        </axis>
      </joint>
      
      
      <link name="front_left_wheel_link">
      
        <inertial>
          <pose>0 0 0 0 0 0</pose>
          <inertia>
            <ixx>0.007171</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.001361</iyy>
            <iyz>0</iyz>
            <izz>0.007171</izz>
          </inertia>
          <mass>0.1</mass>
        </inertial>
        
        
        <collision name="front_left_wheel_collision">
          <pose>0.4775 0.2325 0 -1.5707 0 0</pose>
          <geometry>
            <cylinder>
              <radius>0.165</radius>
              <length>0.140</length>
            </cylinder>
          </geometry>
        </collision>
        
        <visual name="front_left_wheel_visual">
          <pose>0.4775 0.2325 0 -1.5707 0 0</pose>
          <geometry>
            <cylinder>
              <radius>0.165</radius>
              <length>0.140</length>
            </cylinder>
          </geometry>
        </visual>
      </link>
      
      <!--sensor-->
      <joint name="scan_joint" type="fixed">
        <parent>chassis_link</parent>
        <child>base_scan</child>
        <pose>0.2388 0.08 0.270 0 0 0</pose>
      </joint>
  
      <link name="base_scan">
        <inertial>
          <pose>0.2388 0.08 0.270 0 0 0</pose>
          <inertia>
            <ixx>0.001</ixx>
            <ixy>0.000</ixy>
            <ixz>0.000</ixz>
            <iyy>0.001</iyy>
            <iyz>0.000</iyz>
            <izz>0.001</izz>
          </inertia>
          <mass>0.125</mass>
        </inertial>

        <collision name="lidar_sensor_collision">
          <pose>0.2388 0.08 0.270 0 0 0</pose>
          <geometry>
            <cylinder>
              <radius>0.0508</radius>
              <length>0.055</length>
            </cylinder>
          </geometry>
        </collision>

        <visual name="lidar_sensor_visual">
          <pose>0.2388 0.08 0.270 0 0 0</pose>
          <geometry>
            <mesh>
              <uri>model://turtlebot3_waffle/meshes/lds.dae</uri>
              <scale>0.001 0.001 0.001</scale>
            </mesh>
          </geometry>
        </visual>

        <!--sensor name="hls_lfcd_lds" type="ray"-->
        <sensor name="lidar_sensor" type="ray">
          <always_on>true</always_on>
          <visualize>true</visualize>
          <pose>0.2388 0.08 0.270 0 0 0</pose>
          <update_rate>5</update_rate>
          <ray>
            <scan>
              <horizontal>
                <samples>360</samples>
                <resolution>1.000000</resolution>
                <min_angle>0.000000</min_angle>
                <max_angle>6.280000</max_angle>
              </horizontal>
            </scan>
            <range>
              <min>0.20000</min>
              <max>30</max>
              <resolution>0.015000</resolution>
            </range>
            <noise>
              <type>gaussian</type>
              <mean>0.0</mean>
              <stddev>0.01</stddev>
            </noise>
          </ray>
          <plugin name="laserscan_realistic_plugin" filename="libgazebo_ros_ray_sensor.so">
            <ros>
                <!-- <namespace>/tb3</namespace> -->
                <remapping>~/out:=scan</remapping>
            </ros>
            <output_type>sensor_msgs/LaserScan</output_type>
            <frame_name>base_scan</frame_name>
          </plugin>
        </sensor>
      </link>
      
      
           
      <plugin name='ackermann_drive_plugin' filename='libgazebo_ros_ackermann_drive.so'>

        <ros>
          <remapping>distance:=distance_demo</remapping>
        </ros>

        <update_rate>100.0</update_rate>

        <!-- wheels -->
        <front_left_joint>front_left_wheel_joint</front_left_joint>
        <front_right_joint>front_left_wheel_joint</front_right_joint>
        <rear_left_joint>rear_left_wheel_joint</rear_left_joint>
        <rear_right_joint>rear_right_wheel_joint</rear_right_joint>
        <left_steering_joint>front_left_steer_joint</left_steering_joint>
        <right_steering_joint>front_right_steer_joint</right_steering_joint>
        <!--steering_wheel_joint>steering_joint</steering_wheel_joint-->

        <!-- Max absolute steer angle for tyre in radians-->
        <!-- Any cmd_vel angular z greater than this would be capped -->
        <max_steer>0.6458</max_steer>

        <!-- Max absolute steering angle of steering wheel -->
        <max_steering_angle>7.85</max_steering_angle>

        <!-- Max absolute linear speed in m/s -->
        <max_speed>20</max_speed>

        <!-- PID tuning -->
        <left_steering_pid_gain>180 0 30</left_steering_pid_gain>
        <left_steering_i_range>0 0</left_steering_i_range>
        <right_steering_pid_gain>180 0 30</right_steering_pid_gain>
        <right_steering_i_range>0 0</right_steering_i_range>
        <linear_velocity_pid_gain>1000 0 1</linear_velocity_pid_gain>
        <linear_velocity_i_range>0 0</linear_velocity_i_range>

        <!-- output -->
        <publish_odom>true</publish_odom>
        <publish_odom_tf>true</publish_odom_tf>
        <publish_wheel_tf>true</publish_wheel_tf>
        <publish_distance>true</publish_distance>

        <odometry_frame>odom</odometry_frame>
        <robot_base_frame>base_link</robot_base_frame>
        
         <odometry_source>2</odometry_source>

      </plugin>


    </model>

  </world>
</sdf>
